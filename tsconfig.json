{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "types": [
      "vite/client",
    ],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "baseUrl": ".",
    "paths": {
      "parts:*": [
        "src/parts/*"
      ],
      "data:*": [
        "src/data/*"
      ],
      "base:*": [
        "src/parts/base/*"
      ],
      "blocks:*": [
        "src/parts/blocks/*"
      ],
      "elements:*": [
        "src/parts/elements/*"
      ],
      "sections:*": [
        "src/parts/sections/*"
      ],
      "library:*": [
        "src/scripts/library/*",
      ],
      "project:*": [
        "src/scripts/project/*"
      ],
      "public:*": [
        "public/*"
      ]
    },
  }
}